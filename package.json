    "@types/cheerio": "^0.22.22",
    "@types/jest": "^26.0.15",
    "@types/node-fetch": "^2.5.7",
    "@types/puppeteer": "^5.4.0",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
